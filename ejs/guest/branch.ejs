<%
let id = url.split('/')[1]
let data = await db.query(`SELECT * FROM system.branch where id = '${id}'`)
element = data[0]
let parsedQuery = element['address'].replace(/ /g, '%20')
%>
<div class="news_item">
    <%= element['address'] %>
    <hr>
    <iframe style="width: 100%; height: 400px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" id="gmap_canvas"
            src="https://maps.google.com/maps?width=784&amp;height=474&amp;hl=en&amp;q=<%=parsedQuery%>&amp;t=&amp;z=17&amp;ie=UTF8&amp;iwloc=B&amp;output=embed">
    </iframe>
</div>